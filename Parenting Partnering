#include<bits/stdc++.h>
using namespace std;
int main()
{

    int t;
    cin>>t;
    int c=0;
    while(t--)
    {
    string s="";
        int n;
        cin>>n;
        int a[n][2];
        int x[n]={0};
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<2;j++)
            {
                cin>>a[i][j];
                if(j==0 && i==0)
                {
                    s.append("C");
                }
                else if(i>0 && j==1)
                {
                    if(a[i][0]< a[i-1][1] && a[i][j]> a[i-1][0])
                    {
                        if(x[i-1]==1)
                        {
                            s="IMPOSSIBLE";
                            break;
                        }
                        else if (x[i-1]==0)
                        {
                            if(s.back()=='J')
                                s.append("C");
                            else
                                s.append("J");
                            x[i]=1;
                        }
                    }
                    else if(a[i][0]>=a[i-1][1])
                    {
                        if(s.back()=='J')
                                s.append("J");
                            else
                                s.append("C");

                    }
                    else if(a[i][0]<a[i-1][1] && a[i][j]<a[i-1][0])
                    {
                             if(s.back()=='J')
                                s.append("C");
                            else
                                s.append("J");
                    }



                }

            }
        }
            cout<<"Case #"<<c+1<<": "<<s<<endl;
            c++;
        }




    }
